{
  "gpus" : 1 ,
  "mode":"development",
  "preprocessing":
  {
    "basic_time": 1e-2,
    "use_scaling":false,
    "feature_range" : [-1,1],
    "use_whitening" : false,
    "rnn":
    {
      "feature_names" : ["I", "Q"],
      "series_offset" : 16
    },
    "ae":
    {
      "rbw_set" : [125e3],
      "rbw" : 63e3,
      "feature_names":["P"] ,
      "use_noise":false,
      "window": "hamming"
    }
  },
  "learning":
  {
    "rnn": {
      "models_dir": "model/rnn",
      "num_epochs": 1,
      "batch_size": 512,
      "lr": 0.001,
      "validation_split": 0.1,
      "num_clusters": 16,
      "cov_types": [
        "diag",
        "spherical"
      ],
      "seq_input_length": 16,
      "seq_output_length": 8,
      "input_padding": "post",
      "output_padding": "pre"
    },
    "ae":
    {
      "models_dir": "model/ae",
      "num_epochs":25,
      "batch_size":32,
      "lr":0.001,
      "validation_split":0.2,
      "block_length":128,
      "block_shape":[300, 30]
    }
  },
  "detection":
  {
    "rnn":
    {
      "scores_sample_size":256,
    "sigma":20.0
    },
    "ae":
    {
      "sigma":3
    }
  },
  "cepstrum":
    {
      "median_kernel_size": 11,
      "cepstrum_window_size": 512,
      "rbw": 125e3
    }
}
